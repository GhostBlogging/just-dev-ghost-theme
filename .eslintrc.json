{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "globals": {
    "$": "readonly",
    "mediumZoom": "readonly",
    "GapTheme": "readonly",
    "Vue": "readonly",
    "Prism": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "standard/no-callback-literal": 0
  }
}
